{
    "name": "@nautine/react-nautine",
    "version": "1.0.1",
    "description": "React component for Nautine Logger services.",
    "author": "Szilard Doro <doroszilard@gmail.com>",
    "license": "MIT",
    "private": false,
    "repository": "https://github.com/nautine/react-nautine.git",
    "main": "build/index.js",
    "module": "build/index.modern.js",
    "source": "src/index.ts",
    "engines": {
        "node": ">=10"
    },
    "keywords": [
        "front-end",
        "log",
        "maintenance",
        "react",
        "react-js",
        "react-native",
        "third-party",
        "nautine"
    ],
    "scripts": {
        "build": "microbundle-crl --no-compress --format modern,cjs",
        "start": "microbundle-crl watch --no-compress --format modern,cjs",
        "prepare": "run-s test",
        "lint:verbose": "eslint ./src/**/*.{ts,tsx} --fix",
        "test": "run-s test:unit test:lint test:build",
        "test:build": "run-s build",
        "test:unit": "TZ=UTC cross-env CI=1 react-scripts test --env=jsdom --silent",
        "test:lint": "yarn lint:verbose --quiet",
        "test:watch": "react-scripts test --env=jsdom"
    },
    "peerDependencies": {
        "react": "^16.0.0"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^11.0.4",
        "@testing-library/user-event": "^7.2.1",
        "@types/jest": "^25.1.4",
        "@types/node": "^12.12.38",
        "@types/react": "^16.9.27",
        "@types/react-dom": "^16.9.7",
        "@typescript-eslint/eslint-plugin": "^4.3.0",
        "@typescript-eslint/parser": "^4.3.0",
        "cross-env": "^7.0.2",
        "eslint": "7.2.0",
        "eslint-config-airbnb": "18.2.0",
        "eslint-config-airbnb-typescript": "^11.0.0",
        "eslint-config-prettier": "^6.12.0",
        "eslint-plugin-import": "^2.21.2",
        "eslint-plugin-jest": "^24.0.2",
        "eslint-plugin-jsx-a11y": "^6.3.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.20.0",
        "eslint-plugin-react-hooks": "4.0.0",
        "husky": ">=4",
        "lint-staged": ">=10",
        "microbundle-crl": "^0.13.10",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.1.2",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-scripts": "^3.4.1",
        "typescript": "^3.7.5"
    },
    "files": [
        "build"
    ],
    "dependencies": {
        "axios": "^0.20.0",
        "date-fns": "^2.16.1",
        "stacktrace-js": "^2.0.2",
        "tiny-warning": "^1.0.3"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "yarn test:lint",
            "prettier --write"
        ]
    }
}
